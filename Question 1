#include <iostream>
using namespace std;
#define max 10
class stack{
    int arr[max];
int top1=-1;
public:
void push(int a){
   if (top1>=max-1){
    cout<<"Stack overflow\n";
   } 
   else{
    arr[++top1]=a;
   }
}
void pop(){
    if (top1==-1){
        cout<<"Stack underflow\n";
    }
    else{
        top1--;
    }
}
int peek(){
    if(top1<0){
        cout<<"Stack is empty\n";
        return -1;
    }
    else{
        return arr[top1];
    }
}
bool isEmpty(){
    if(top1==-1){
        return true;
    }
    else{
        return false;
    }
}
int size(){
    return top1+1;
}
void display(){
    if (top1==-1){
        cout<<"Stack is empty"<<endl;
    }
    else{
        for(int i=top1;i>=0;i--){
            cout<<arr[i]<<" ";

        }
    }
}
bool isFull(){
    if(top1==max-1){
        return true;
    }
    else{
        return false;
    }
}
};
int main(){
        stack s;
    s.push(10);
    s.push(20);
    s.push(30);
    s.display();
    cout<<s.peek()<<endl;
    cout<<s.isEmpty()<<endl;
    cout<<s.size()<<endl;
   cout<< s.isFull()<<endl;
    cout<<s.isEmpty()<<endl;


}
